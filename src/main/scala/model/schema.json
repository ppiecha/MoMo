{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome8",
    "definitions": {
        "Welcome8": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "BPM": {
                    "type": "integer"
                },
                "soundFontPath": {
                    "type": "string"
                },
                "tracks": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Track"
                    }
                },
                "author": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "comment": {
                    "type": "string"
                },
                "PPQ": {
                    "type": "integer"
                }
            },
            "required": [
                "BPM",
                "soundFontPath",
                "tracks"
            ],
            "title": "Welcome8"
        },
        "Track": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "channel": {
                    "type": "integer"
                },
                "instrument": {
                    "type": "integer"
                },
                "versions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Version"
                    }
                },
                "active": {
                    "type": "boolean"
                }
            },
            "required": [
                "channel",
                "instrument",
                "versions"
            ],
            "title": "Track"
        },
        "Version": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "notesOn": {
                    "type": "string"
                },
                "timing": {
                    "type": "string"
                },
                "start": {
                    "type": "integer"
                },
                "duration": {
                    "type": "string"
                },
                "velocity": {
                    "type": "null"
                },
                "active": {
                    "type": "boolean"
                }
            },
            "required": [
                "duration",
                "notesOn",
                "timing"
            ],
            "title": "Version"
        }
    }
}
